{
  "name": "notafiscalsp/notafiscal-pdf",
  "description": "Lib simples para gerar PDF's das Notas Fiscais da Prefeitura do Estado de São Paulo",
  "type": "library",
  "license": "MIT",
  "require": {
    "dompdf/dompdf": "^1.0"
  },
  "suggest": {
    "ext-mbstring": "*"
  },
  "keywords": [
    "NFS",
    "Nota Fiscal",
    "Nota do Milhão",
    "Prefeitura São Paulo"
  ],
  "authors": [
    {
      "name": "Kaio Valdemar",
      "email": "profkaio@outlook.com"
    },
    {
      "name": "Marcelo Mendonça da Silva (marceloms163)"
    }
  ],
  "autoload": {
    "psr-4": {
      "NfSPPDF\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "stable"
}